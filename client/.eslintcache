 <Input\n              defaultValue={users.name}\n              onChange={(e) => setName(e.target.value) }\n              style={{ width: 300 }}\n            />\n          </h5>\n        </Row>\n        <Row>\n          <h5>\n            Edit Email :{\" \"}\n            <Input\n              defaultValue={users.email}\n              onChange={(e) => setEmail(e.target.value)}\n              style={{ width: 300, marginLeft: 5 }}\n            />\n          </h5>\n        </Row>\n        <Row>\n        <h5>Edit Role : </h5>\n        <Radio.Group buttonStyle=\"solid\" defaultValue={users.role} onChange={(e) => setRole(e.target.value)} style={ { marginLeft: 20}}>\n            <Space>\n            <Radio.Button value=\"user\">User</Radio.Button>\n            <Radio.Button value=\"admin\">Admin</Radio.Button>\n            </Space>\n        </Radio.Group>\n        </Row>\n      </Modal>\n    </div>\n  );\n};\n\nexport default EditInfo;\n","/Users/jack/Desktop/Peppermint/client/src/component/newsletter/Edit.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/admin/ResetPassword.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/OpenTicket.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/TicketStats.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/UnissuedTicket.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/NewTicket.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/todo/ListTodo.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/todo/CreateTodo.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/note/ListNote.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/ViewTicket.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/note/EditNote.js",[],"/Users/jack/Desktop/Peppermint/client/src/component/ticket/Transfer.js",[],{"ruleId":"179","replacedBy":"180"},{"ruleId":"181","replacedBy":"182"},{"ruleId":"183","severity":1,"message":"184","line":130,"col