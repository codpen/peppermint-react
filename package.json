{
  "name": "project",
  "version": "0.1.5",
  "description": "Ticket system",
  "main": "server.js",
  "bin": {
    "project": "server.js"
  },
  "pkg": {
    "assets": [
      "build/**/*",
      "data/*",
      "src/**/*",
      "images/*",
      "config/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node12-macos-x64",
      "node10-win-x64"
    ]
  },
  "scripts": {
    "seed": "prisma db seed --preview-feature",
    "migrate": "prisma migrate dev --preview-feature",
    "generate": "prisma generate",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
  },
  "keywords": [],
  "author": "Jack Andrews",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "os-utils": "0.0.14",
    "socket.io": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "prisma": "^2.21.2"
  }
}
